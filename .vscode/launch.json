  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    },
    {
      "preLaunchTask": "quarkus:dev",
      "type": "java",
      "request": "attach",
      "hostName": "localhost",
      "name": "Quarkus dev",
      "port": 5005
    },
    {
      "type": "pwa-chrome",
      "name": "Chrome",
      "request": "launch",
      "url": "http://localhost:8080/index.html"
    },
    {
      "command": "ng build --source-map --watch --configuration development",
      "name": "Angular watch",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
